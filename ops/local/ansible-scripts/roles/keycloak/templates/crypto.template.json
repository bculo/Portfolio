{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "CryptoDatabase": "Host=localhost;Port=5433;Database=Crypto;User Id=postgres;Password=florijan;"
  },
  "CryptoPriceApiOptions": {
    "HeaderKey": "authorization",
    "ApiKey": "<<SECRET>>",
    "BaseUrl": "https://min-api.cryptocompare.com/data/",
    "Currency": "USD",
    "RetryNumber": 3,
    "Timeout": 60
  },
  "CryptoInfoApiOptions": {
    "RetryNumber": 3,
    "Timeout": 60,
    "HeaderKey": "X-CMC_PRO_API_KEY",
    "ApiKey": "<<SECRET>>",
    "BaseUrl": "https://pro-api.coinmarketcap.com/v2/cryptocurrency/"
  },
  "QueueOptions": {
    "Prefix": "Crypto",
    "Address": "amqp://rabbitmquser:rabbitmqpassword@localhost:5672",
    "Temporary": false
  },
  "IdentifierHasher": {
    "Salt": "KVO9yy1oO5j",
    "HashLength": 12
  },
  "RedisOptions": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "crypto"
  },
  "SagaTimeoutOptions": {
    "TimeoutCryptoAddInSeconds": 15
  },
  "ApiVersion": {
    "MajorVersion": 1,
    "MinorVersion": 1
  },
  "KeycloakOptions": {
    "ApplicationName": "Crypto.API",
    "RealmName": "PortfolioRealm",
    "AuthorizationServerUrl": "http://localhost:8080/realms/PortfolioRealm"
  },
  "AuthOptions": {
    "PublicRsaKey": "{{ public_keycloak_key }}",
    "ValidIssuer": "http://localhost:8080/realms/PortfolioRealm",
    "ValidateAudience": false,
    "ValidateIssuer": true,
    "ValidateIssuerSigningKey": true,
    "ValidateLifetime": true
  }
